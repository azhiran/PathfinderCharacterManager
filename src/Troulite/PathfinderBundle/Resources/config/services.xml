<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="troulite_pathfinder.characterbonuses.class">
            Troulite\PathfinderBundle\Services\CharacterBonuses
        </parameter>
        <parameter key="troulite_pathfinder.abilitytools.class">Troulite\PathfinderBundle\Services\AbilityTools
        </parameter>

    </parameters>

    <services>
        <service id="troulite_pathfinder.character_bonuses" class="%troulite_pathfinder.characterbonuses.class%">
        </service>
        <service id="troulite_pathfinder.ability_tools" class="%troulite_pathfinder.abilitytools.class%">
        </service>
    </services>
</container>
